{
  "name": "github-automation",
  "version": "3.2.9",
  "description": "oclif example Hello World CLI",
  "author": "Jorge Saud @Giorgiosaud",
  "bin": {
    "github-automation": "./bin/run"
  },
  "homepage": "https://github.com/Giorgiosaud/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Giorgiosaud/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.8.2",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-plugins": "^2.4.7",
    "@types/js-yaml": "^4.0.5",
    "axios": "^1.3.6",
    "colorts": "^0.1.63",
    "dotenv": "^16.0.3",
    "js-yaml": "^4.1.0",
    "libsodium-wrappers": "^0.7.11",
    "octokit": "^2.0.14"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@oclif/test": "^2.3.17",
    "@types/chai": "^4.3.4",
    "@types/jest": "^29.5.1",
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "axios-mock-adapter": "^1.21.4",
    "chai": "^4.3.7",
    "eslint": "^8.39.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "eslint-plugin-unicorn": "^46.0.0",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-worker": "^29.5.0",
    "mocha": "^10.2.0",
    "oclif": "^3.8.1",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "oclif": {
    "bin": "github-automation",
    "dirname": "github-automation",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/Giorgiosaud/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
