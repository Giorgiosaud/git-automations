{
  "name": "git-automated-actions",
  "version": "1.0.1",
  "description": "github automator",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint --ext .ts src",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "test": "npm i -g && github-api",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "bin": {
    "github-api": "./lib/index.js"
  },
  "author": "Jorge Saud",
  "license": "MIT",
  "dependencies": {
    "@types/clear": "^0.1.0",
    "@types/figlet": "^1.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/node-fetch": "^2.5.6",
    "caporal": "^1.3.0",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "figlet": "^1.3.0",
    "fs-extra": "^9.0.0",
    "inquirer": "^7.1.0",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "tweetsodium": "^0.0.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
