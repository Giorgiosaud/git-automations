{
  "name": "github-automation",
  "version": "3.1.12",
  "description": "an automation tool for github",
  "author": "Jorge Saud @Giorgiosaud",
  "bin": {
    "github-automation": "./bin/run"
  },
  "homepage": "https://github.com/Giorgiosaud/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "Giorgiosaud/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.9.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-plugins": "^2.1.0",
    "@octokit/core": "^3.6.0",
    "@octokit/types": "^6.34.0",
    "@types/libsodium-wrappers": "^0.7.9",
    "colorts": "^0.1.63",
    "jest-config": "^28.1.1",
    "fs-extra": "^10.1.0",
    "dotenv": "^16.0.1",
    "axios": "^0.27.2",
    "libsodium-wrappers": "^0.7.10"
  },
  "devDependencies": {
    "@jest/types": "^28.1.1",
    "@oclif/test": "^2.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.1",
    "@types/node": "^16.11.38",
    "axios-mock-adapter": "^1.21.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11.1.0",
    "jest": "^28.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "mock-stdin": "^1.0.0",
    "oclif": "^3.0.1",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "oclif": {
    "bin": "github-automation",
    "dirname": "github-automation",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm lint",
    "prepack": "pnpm build && oclif manifest && oclif readme",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/Giorgiosaud/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "sonar-cloud-reporter.xml",
    "indent": 4
  },
  "types": "dist/index.d.ts"
}
