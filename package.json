{
  "name": "github-automation",
  "description": "a github automation toolbox",
  "version": "2.0.0",
  "author": "Jorge Saud @Giorgiosaud",
  "bin": {
    "github-automation": "./bin/run"
  },
  "bugs": "https://github.com/Giorgiosaud/github-automation/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/fs-extra": "8.1.0",
    "@types/node-fetch": "^2.5.12",
    "dotenv": "^10.0.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "node-fetch": "^2.6.1",
    "sinon": "^11.1.2",
    "tslib": "^1",
    "tweetsodium": "^0.0.5"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/Giorgiosaud/github-automation",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "github-automation",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "Giorgiosaud/github-automation",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
